<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>Jiisp</display-name>
	<welcome-file-list>
		<welcome-file>Site</welcome-file>
	</welcome-file-list>
	<error-page>
		<error-code>404</error-code>
		<location>/Site?action=404</location>
	</error-page>
	<servlet>
		<description></description>
		<display-name>SiteController</display-name>
		<servlet-name>SiteController</servlet-name>
		<servlet-class>controllers.SiteController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SiteController</servlet-name>
		<url-pattern>/Site</url-pattern>
	</servlet-mapping>
</web-app>